{
  "name": "astron.libts",
  "version": "0.1.0",
  "description": "An open source implementation of the Astron real-time object networking protocol.",
  "main": "dist/index.js",
  "types": "dist/@types",
  "repository": {
    "type": "git",
    "url": "https://github.com/Max-Rodriguez/astron.libts.git"
  },
  "keywords": [
    "astron",
    "libastron",
    "otp",
    "networking",
    "mmo",
    "distributed"
  ],
  "author": "Max Rodriguez <max@vulkray.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Max-Rodriguez/astron.libts/issues"
  },
  "homepage": "https://github.com/Max-Rodriguez/astron.libts",
  "scripts": {
    "build": "yarn tsc -p tsconfig.build.json",
    "tests": "yarn tsc -p tsconfig.tests.json",
    "asbuild:debug": "asc src/index.ts --target debug",
    "asbuild:release": "asc src/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "test": "node tests",
    "start": "npx serve ."
  },
  "devDependencies": {
    "@types/node": "^18.15.12",
    "typedoc": "^0.24.4",
    "typescript": "^5.0.4",
    "assemblyscript": "^0.27.2"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.27.2"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}